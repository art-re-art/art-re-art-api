{
  "private": true,
  "scripts": {
    "start": "concurrently -r 'npm:start:django' 'npm:start:webpack'",
    "start:django": "pipenv run python manage.py runserver",
    "start:webpack": "nodemon --watch webpack.config.js --watch antd.theme.js --exec 'webpack --watch'",
    "build": "webpack --mode 'production'",
    "manage:migrate": "pipenv run python manage.py migrate",
    "manage:makemigrations": "pipenv run python manage.py makemigrations",
    "manage:createsuperuser": "pipenv run python manage.py createsuperuser",
    "pg:pull": "dropdb --if-exists art-re-art; heroku pg:pull postgresql-colorful-51974 art-re-art --app art-re-art",
    "format": "concurrently \"prettier --write client/static_src/**/*.js\" \"prettier --write client/static_src/**/*.less\" \"black --exclude /node_modules/ .\""
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "concurrently": "^4.1.1",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.1.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "antd": "^3.20.5",
    "axios": "^0.19.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-ga": "^2.6.0",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.1",
    "webfontloader": "^1.6.28"
  }
}
