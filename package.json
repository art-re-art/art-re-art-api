{
  "private": true,
  "scripts": {
    "start": "concurrently 'npm:start:django' 'npm:start:webpack'",
    "start:django": "pipenv run python manage.py runserver",
    "start:webpack": "nodemon --watch webpack.config.js --watch antd.theme.js --exec 'webpack --watch'",
    "build": "webpack --mode 'production'",
    "manage:migrate": "pipenv run python manage.py migrate",
    "manage:makemigrations": "pipenv run python manage.py makemigrations",
    "manage:createsuperuser": "pipenv run python manage.py createsuperuser",
    "pg:pull": "dropdb --if-exists art-re-art; heroku pg:pull postgresql-colorful-51974 art-re-art --app art-re-art",
    "format": "concurrently \"prettier --write client/static_src/**/*.js\" \"prettier --write client/static_src/**/*.less\" \"black --exclude /node_modules/ .\""
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-import": "^1.12.2",
    "concurrently": "^4.1.2",
    "css-loader": "^6.7.3",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "less": "^3.10.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^7.0.2",
    "prettier": "^1.18.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "react": "^16.11.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.11.0",
    "react-ga": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "webfontloader": "^1.6.28"
  }
}
